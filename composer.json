{
	"name": "bogosoft/cornpop",
	"description": "A PSR-compliant nano-framework for generating responses for HTTP requests.",
	"keywords": ["psr", "kernel", "http", "byoi"],
	"license": "MIT",
	"require": {
		"bogosoft/configuration": "^1",
		"php": ">=7.1",
		"psr/container": "^1",
		"psr/http-message": "^1",
		"psr/http-server-handler": "^1",
		"psr/http-server-middleware": "^1",
		"psr/log": "^1"
	},
	"require-dev": {
		"guzzlehttp/psr7": "^1.6",
		"phpunit/phpunit": "^8"
	},
	"autoload": {
		"psr-4": {
			"Bogosoft\\CornPop\\": "src/",
			"Tests\\": "tests/"
		}
	}
}
